{
  "name": "markmoji",
  "version": "1.0.0",
  "description": "emoji for anything",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "rm -rf dist && parcel build ./index.html manifest.json --no-source-maps && tar -cf markmoji.zip dist"
  },
  "dependencies": {
    "mithril": "1.1.6",
    "moji-translate": "latest",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.10.2",
    "parcel-plugin-web-extension": "^1.4.0"
  }
}
